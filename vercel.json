{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "css/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "js/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "img/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "imagen/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "portfolio/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "guias/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "user-dashboard/dist/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "dashboard-ts/dist/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "favicon.svg",
      "use": "@vercel/static"
    },
    {
      "src": "favicon.ico",
      "use": "@vercel/static"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/dashboard/assets/(.+\\.css)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    },
    {
      "source": "/dashboard-ts/assets/(.+\\.css)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    },
    {
      "source": "/admin/assets/(.+\\.css)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    },
    {
      "source": "/(.+\\.css)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    },
    {
      "source": "/dashboard/assets/(.+\\.js)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/dashboard-ts/assets/(.+\\.js)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/admin/assets/(.+\\.js)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/(.+\\.js)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/(.+\\.svg)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/svg+xml"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "/user-dashboard/dist/index.html"
    },
    {
      "source": "/dashboard/assets/:path*",
      "destination": "/user-dashboard/dist/assets/:path*"
    },
    {
      "source": "/dashboard/:path*",
      "destination": "/user-dashboard/dist/:path*"
    },
    {
      "source": "/dashboard-ts",
      "destination": "/dashboard-ts/dist/index.html"
    },
    {
      "source": "/dashboard-ts/assets/:path*",
      "destination": "/dashboard-ts/dist/assets/:path*"
    },
    {
      "source": "/dashboard-ts/:path*",
      "destination": "/dashboard-ts/dist/:path*"
    },
    {
      "source": "/admin",
      "destination": "/dashboard-ts/dist/index.html"
    },
    {
      "source": "/admin/assets/:path*",
      "destination": "/dashboard-ts/dist/assets/:path*"
    },
    {
      "source": "/admin/:path*",
      "destination": "/dashboard-ts/dist/:path*"
    },
    {
      "source": "/favicon.svg",
      "destination": "/dashboard-ts/dist/favicon.svg"
    },
    {
      "source": "/favicon.ico",
      "destination": "/favicon.ico"
    },
    {
      "source": "/script.js",
      "destination": "/script.js"
    },
    {
      "source": "/styles.css",
      "destination": "/styles.css"
    },
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/css/:path*",
      "destination": "/css/:path*"
    },
    {
      "source": "/js/:path*",
      "destination": "/js/:path*"
    },
    {
      "source": "/img/:path*",
      "destination": "/img/:path*"
    },
    {
      "source": "/imagen/:path*",
      "destination": "/imagen/:path*"
    },
    {
      "source": "/portfolio/:path*",
      "destination": "/portfolio/:path*"
    },
    {
      "source": "/guias/:path*",
      "destination": "/guias/:path*"
    },
    {
      "source": "/:file(.*)\\.html",
      "destination": "/:file.html"
    }
  ]
}
